{
    "name": "johnkary/php-skeleton",
    "description": "Base project skeleton for a PHP 7.2+ project",
    "keywords": ["PHP", "example", "project", "skeleton"],
    "homepage": "https://github.com/johnkary/skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "John Kary",
            "email": "john@johnkary.net",
            "homepage": "https://johnkary.net",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2",
        "phpunit/phpunit": "^8.0",
        "symfony/console": "^5.2"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Acme\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Acme\\Tests\\": "tests/"
        }
    }
}
